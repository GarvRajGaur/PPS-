#include <stdio.h>
int main(){
  int a,b,c;
printf("enter the numbers:");
scanf("%d %d",&a,&b);
printf("\n before swapping");
//numbers before swapping
printf("\n a=%d",a);
printf("\n b=%d",b);
//calculation
c=a;
a=b;
b=c;
printf("\n after swapping");
//output
printf("\n a=%d",a);
printf("\n b=%d",b);

        return 0;
} 
